{
  "name": "astro-static-headers",
  "description": "Astro integration that captures headers and redirects during prerender and writes platform-specific config (Netlify, Cloudflare, Vercel).",
  "version": "1.0.1",
  "type": "module",
  "author": "Adam Bouqdib <adam@abemedia.co.uk>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abemedia/astro-static-headers.git"
  },
  "bugs": "https://github.com/abemedia/astro-static-headers/issues",
  "homepage": "https://github.com/abemedia/astro-static-headers#readme",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "tsdown src/index.ts src/middleware.ts",
    "prepack": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "biome format --write . && prettier --write --cache \"**/*.{md,yml,yaml,astro}\"",
    "lint": "biome lint --write .",
    "check": "biome check . && tsc --noEmit"
  },
  "keywords": [
    "astro-integration",
    "astro",
    "withastro",
    "headers",
    "redirects",
    "cloudflare",
    "netlify",
    "vercel"
  ],
  "engines": {
    "node": "^20.3.0 || >=22.0.0"
  },
  "packageManager": "pnpm@10.17.1",
  "peerDependencies": {
    "astro": ">=5.0.0"
  },
  "dependencies": {
    "@astrojs/underscore-redirects": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^20.19.25",
    "astro": "^5.16.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "lint-staged": {
    "*": "biome check --write --files-ignore-unknown=true --no-errors-on-unmatched",
    "*.{md,yml,yaml,astro}": "prettier --write"
  },
  "publishConfig": {
    "provenance": true
  }
}
